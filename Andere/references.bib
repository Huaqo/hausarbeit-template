@article{dzudzek_urbane_2020,
	title = {Urbane {Gesundheitsgerechtigkeit}: Öko-sozialepidemiologische {Forschungsperspektiven} für eine {Kritische} {Stadtgeographie} verkörperter {Ungleichheiten}. {What} critical urban geography can learn from ecosocial epidemiology for researching embodied inequalities},
